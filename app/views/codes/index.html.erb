<h1>Listing codes</h1>

<table>
  <tr>
    <th>Printable</th>
    <th>User</th>
    <th>App</th>
    <th>Code</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @codes.each do |code| %>
  <tr>
    <td><b><%= code.to_s %></b></td>
    <td><%= code.user %></td>
    <td><%= code.app %></td>
    <td><%= code.code %></td>
    <td><%= link_to 'Show', code %></td>
    <td><%= link_to 'Edit', edit_code_path(code) %></td>
    <td><%= link_to 'Destroy', code, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Code', new_code_path %>
